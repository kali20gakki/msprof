{
    "Rules":[
        {
            "RuleName":"ClipFusion_with_attr",
            "OuterInputs":[
                {
                    "name":"input0"
                },
                {
                    "name":"input1"
                }
            ],
            "OuterOutputs":[
                {
                    "name":"output0"
                }
            ],
            "OriginGraph":{
                "Nodes":[
                    {
                        "name":"node1",
                        "type":[
                            "min"
                        ]
                    },
                    {
                        "name":"node2",
                        "type":[
                            "max"
                        ]
                    },
                    {
                        "name":"output0",
                        "type":[
                            "max"
                        ]
                    }
                ],
                "Edges":[
                    {
                        "src":"input0",
                        "dst":"node1:0"
                    },
                    {
                        "src":"input1",
                        "dst":"node1:1"
                    },
                    {
                        "src":"node1:0",
                        "dst":"node2:0"
                    },
                    {
                        "src":"node2:0",
                        "dst":"output0"
                    }
                ]
            },
            "FusionGraph":{
                "Nodes":[
                    {
                        "name":"node3",
                        "type":[
                            "clip"
                        ]
                    }
                ],
                "Edges":[
                    {
                        "src":"input0",
                        "dst":"node3:0"
                    },
                    {
                        "src":"input1",
                        "dst":"node3:1"
                    },
                    {
                        "src":"node3:0",
                        "dst":"output0"
                    }
                ],
                "Attrs":[
                    {
                        "attr":"node3.attr1",
                        "expr":"=",
                        "value":"node1.attr1"
                    }
                ]
            }
        }
    ]
}